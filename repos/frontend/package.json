{
  "name": "@lkns/frontend",
  "description": "Frontend for likeness app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "Lance Tipton",
    "email": "lancetipton04@gmail.com",
    "url": "https://github.com/lancetipton"
  },
  "bugs": {
    "url": "https://github.com/lancetipton/likeness/issues",
    "email": "lancetipton04@gmail.com"
  },
  "scripts": {
    "build": "NODE_ENV=production vite build --clearScreen false -c configs/vite.config.ts",
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "depcheck": "depcheck --ignores=\"@*/*,@likeness,@lkns\" --ignore-patterns=build,dist,.*,coverage,temp,*jest*",
    "preview": "vite -c configs/vite.config.ts preview",
    "test": "vitest -c configs/vite.config.ts",
    "test:ui": "vitest -c configs/vite.config.ts --ui",
    "serve": "NODE_ENV=production serve -p 19019 dist",
    "start": "VITE_CJS_TRACE=true NODE_ENV=local vite --clearScreen false -c configs/vite.config.ts",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@keg-hub/cli-utils": "0.9.0",
    "@keg-hub/jsutils": "10.0.0",
    "@keg-hub/parse-config": ">=2.1.0",
    "@redux-devtools/extension": "3.3.0",
    "@reduxjs/toolkit": "2.0.1",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@types/node": "20.11.0",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react-swc": "3.5.0",
    "@vitest/ui": "1.2.0",
    "autoprefixer": "10.4.16",
    "comlink": "4.4.1",
    "esbuild-register": "3.5.0",
    "happy-dom": "13.0.4",
    "million": "2.6.4",
    "postcss": "8.4.33",
    "react-error-boundary": "4.0.12",
    "react-icons": "4.12.0",
    "react-redux": "9.1.0",
    "react-router-dom": "6.21.2",
    "redux": "5.0.1",
    "serve": "14.2.1",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vite-plugin-comlink": "3.0.5",
    "vite-plugin-svgr-component": "1.0.1",
    "vite-tsconfig-paths": "4.2.3",
    "vitest": "1.2.0"
  }
}
