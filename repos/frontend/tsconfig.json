{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "declaration": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["vite/client", "vitest/globals", "@testing-library/jest-dom"],
    "paths": {
      "@CPAF/actions": ["src/actions"],
      "@CPAF/actions/*": ["src/actions/*"],
      "@CPAF/assets": [ "src/assets" ],
      "@CPAF/assets/*": [ "src/assets/*" ],
      "@CPAF/components": ["src/components"],
      "@CPAF/components/*": ["src/components/*"],
      "@CPAF/constants": [ "src/constants" ],
      "@CPAF/constants/*": [ "src/constants/*" ],
      "@CPAF/contexts": [ "src/contexts" ],
      "@CPAF/contexts/*": [ "src/contexts/*" ],
      "@CPAF/dispatchers": [ "src/store/dispatchers.ts" ],
      "@CPAF/dispatchers/*": [ "src/store/dispatchers.ts" ],
      "@CPAF/hooks": [ "src/hooks" ],
      "@CPAF/hooks/*": [ "src/hooks/*" ],
      "@CPAF/pages": ["src/pages"],
      "@CPAF/pages/*": ["src/pages/*"],
      "@CPAF/router": [ "src/router" ],
      "@CPAF/router/*": [ "src/router/*" ],
      "@CPAF/reducers": [ "src/reducers" ],
      "@CPAF/reducers/*": [ "src/reducers/*" ],
      "@CPAF/services": [ "src/services" ],
      "@CPAF/services/*": [ "src/services/*" ],
      "@CPAF/src": ["src"],
      "@CPAF/src/*": ["src/*"],
      "@CPAF/store": [ "src/store" ],
      "@CPAF/store/*": [ "src/store/*" ],
      "@CPAF/theme": [ "src/theme" ],
      "@CPAF/theme/*": [ "src/theme/*" ],
      "@CPAF/utils": ["src/utils"],
      "@CPAF/utils/*": ["src/utils/*"],
      "@CPAF/workers": ["src/workers"],
      "@CPAF/workers/*": ["src/workers/*"],
      "@keg-hub/jsutils": ["node_modules/@keg-hub/jsutils/build/esm"],
      "@keg-hub/jsutils/*": ["node_modules/@keg-hub/jsutils/build/esm/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
