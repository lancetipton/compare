{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "declaration": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["vite/client"],
    "paths": {
      "@LKT/actions": ["src/actions"],
      "@LKT/actions/*": ["src/actions/*"],
      "@LKT/assets": [ "src/assets" ],
      "@LKT/assets/*": [ "src/assets/*" ],
      "@LKT/components": ["src/components"],
      "@LKT/components/*": ["src/components/*"],
      "@LKT/constants": [ "src/constants" ],
      "@LKT/constants/*": [ "src/constants/*" ],
      "@LKT/hooks": [ "src/hooks" ],
      "@LKT/hooks/*": [ "src/hooks/*" ],
      "@LKT/services": [ "src/services" ],
      "@LKT/services/*": [ "src/services/*" ],
      "@LKT/src": ["src"],
      "@LKT/src/*": ["src/*"],
      "@LKT/store": [ "src/store" ],
      "@LKT/store/*": [ "src/store/*" ],
      "@LKT/styles": [ "src/styles" ],
      "@LKT/styles/*": [ "src/styles/*" ],
      "@LKT/utils": ["src/utils"],
      "@LKT/utils/*": ["src/utils/*"],
      "@keg-hub/jsutils": ["node_modules/@keg-hub/jsutils/build/esm"],
      "@keg-hub/jsutils/*": ["node_modules/@keg-hub/jsutils/build/esm/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
